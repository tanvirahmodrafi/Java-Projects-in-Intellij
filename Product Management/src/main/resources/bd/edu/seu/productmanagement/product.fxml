<AnchorPane fx:id="productTableView" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bd.edu.seu.productmanagement.controller.ProductController">
