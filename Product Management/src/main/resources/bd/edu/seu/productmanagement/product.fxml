      <Button layoutX="273.0" layoutY="298.0" mnemonicParsing="false" onAction="#saveAction" text="Button" />
      <TextField fx:id="productName" layoutX="88.0" layoutY="200.0" promptText="Product Name" />
      <TextField fx:id="productPrice" layoutX="88.0" layoutY="267.0" promptText="Product price" />
